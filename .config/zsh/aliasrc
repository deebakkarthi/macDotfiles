#!/bin/zsh
alias ta="tmux new-session -A -s main"
alias config="/usr/bin/git --git-dir=$HOME/.local/src/dotfiles --work-tree=$HOME"
alias song-dl='noglob yt-dlp -x  --embed-thumbnail --audio-quality 0 --audio-format mp3 -o "%(artist)s - %(title)s.%(ext)s" --parse-metadata "%(release_year)s:%(meta_date)s" --add-metadata'
alias c="cd $COLLEGE_DIR"
alias s="cd $HOME/Documents/Drive/sb/"
alias ls="eza"
alias grep="grep --color=always"
alias conda_init="source $HOME/.local/bin/conda_init"
alias sva=". sva.sh"
